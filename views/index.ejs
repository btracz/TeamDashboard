            <div class="form-group">
              <div class="checkbox">
                <label>
                  <input type="checkbox"> Bloc partagé ?
                </label>
              </div>
            </div>
            <div class="form-group">
              <label>Couleur</label>
              <label class="radio-inline"><input type="radio" name="optradio" checked>Rouge</label>
              <label class="radio-inline"><input type="radio" name="optradio">Bleu</label>
              <label class="radio-inline"><input type="radio" name="optradio">Vert</label>
              <label class="radio-inline"><input type="radio" name="optradio">Jaune</label>
              <label class="radio-inline"><input type="radio" name="optradio">Gris</label>
            </div>
            <div class="form-group">
              <label for="templateInput">Template</label>
              <textarea class="form-control" rows="5" id="templateInput" placeholder="Entrer le template HTML avec ses placeholders"></textarea>
            </div>
            <div class="form-group">
              <div class="checkbox">
                <label>
                  <input type="checkbox"> Header
                </label>
                <label>
                  <input type="checkbox"> Footer
                </label>
              </div>
            </div>
            <label for="templateField1">Champs</label>
            <div class="input-group">
              <input type="text" class="form-control" id="templateField1" placeholder="Saississez un nom pour le champ {0}">
              <span class="input-group-btn">
                <button class="btn btn-default" type="button"><span class="glyphicon glyphicon-plus"
                                                                     aria-hidden="true"></span></button>
              </span>
            </div>
            <button type="submit" class="btn btn-success">Valider</button>
          </form>
        </div>
      </div>
